<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>StartProcess</servlet-name>
		<display-name>StartProcess</display-name>
		<description></description>
		<servlet-class>com.sig.servlet.StartProcess</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>IniciarProceso</servlet-name>
		<display-name>IniciarProceso</display-name>
		<description></description>
		<servlet-class>com.sig.servlet.IniciarProceso</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetProcess</servlet-name>
		<display-name>GetProcess</display-name>
		<description></description>
		<servlet-class>com.sig.servlet.GetProcess</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>StartProcess</servlet-name>
		<url-pattern>/StartProcess</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>IniciarProceso</servlet-name>
		<url-pattern>/IniciarProceso</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetProcess</servlet-name>
		<url-pattern>/GetProcess</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
	    <welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
